<h1>Plots</h1>
<% @plots.each do |plot| %>
<div id="plots-<%= plot.id %>">
        <p>Plot Number: <%= plot.number %></p>
        <p>Plant names:</p>
        <ul>
            <% plot.plants.each do |plant| %>
                <li><%= plant.name %></li>
            <% end %>
        </ul>
    </div>
<% end %>